def BMI_Calculator(BMI):
    if BMI < 18.5:
        message = "Under Weight"
    elif BMI < 24.9:
        message = "Normal"
    elif BMI < 29.9:
        message = "Over Weight"
    elif BMI < 34.9:
        message = "Obesity (Class 1)"
    elif BMI < 39.9:
        message = "Obesity(Class2)"
    else:
        message = "Extreme Obesity"
    return message
#weight should be kg
Weight=70
#Height should be meter
Height=1.80
BMI = Weight / (Height * Height)
print(BMI_Calculator(BMI))
